{
  "name": "workbench",
  "productName": "Workbench",
  "version": "0.1.0",
  "description": "A simple desktop app built on Electron to allow end-users to combine their suite of tools into one location",
  "private": true,
  "main": "src/Workbench/Core/Main.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "electron-pack": "electron-builder",
    "preelectron-pack": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ethan-cook/Workbench.git"
  },
  "author": "Ethan Cook",
  "license": "GPU-3.0-or-later",
  "bugs": {
    "url": "https://github.com/ethan-cook/Workbench/issues"
  },
  "homepage": "./",
  "build": {
    "appId": "nonExistentID",
    "productName": "Workbench",
    "copyright": "Copyright Â© 2019 ${author}",
    "extends": null,
    "mac": {
      "category": "public.app-category.utilities"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "src/Workbench/Core/Main.js"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^7.2.1",
    "react": "^16.12.0",
    "react-app-rewired": "^2.1.5",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.0",
    "typescript": "^3.7.4"
  },
  "devDependencies": {
    "electron": "^7.2.4",
    "electron-builder": "^21.2.0"
  }
}
